{{ header }}
{{ column_left }}
<div id="content">
    <!-- header start -->
    <div class="page-header">
        <div class="container-fluid">
            <div class="pull-right">
                <button type="submit" form="form-messengers-widget" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-success"><i class="fas fa-check"></i></button>
                <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fas fa-share"></i></a>
            </div>
            <h1>{{ heading_title }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    <!-- header end -->
    <!-- content start -->
    <div class="section">
        <div class="container-fluid">
            <!-- alert start -->
            <div class="col-sm-12">
                {% if (error_warning) %}
                    <div class="alert alert-danger"><div class="alert-text"><i class="fas fa-exclamation"></i> {{ error_warning }}</div>
                        <div class="alert-close"><button type="button" class="close" data-dismiss="alert">&times;</button></div>
                    </div>
                {% endif %}
                {% if (success) %}
                    <div class="alert alert-success"><div class="alert-text"><i class="fas fa-check"></i> {{ success }}</div>
                        <div class="alert-close"><button type="button" class="close" data-dismiss="alert">&times;</button></div>
                    </div>
                {% endif %}
            </div>
            <!-- alert end -->
            <!-- tabs start -->
            <div class="col-sm-3">
                <ul class="tabs">
                    <li id="settings-1" class="current"><i class="fas fa-cog"></i>{{ tab_1 }}</li>
                    <li id="settings-2"><i class="fas fa-eye"></i>{{ tab_2 }}</li>
                    <li id="settings-6"><i class="far fa-comment-alt"></i>{{ tab_6 }}</li>
                    <li id="settings-3"><i class="fas fa-link"></i>{{ tab_3 }}</li>
                    <li id="customcss"><i class="fas fa-code"></i>{{ tab_4 }}</li>
                    <li id="settings-5"><i class="fas fa-info-circle"></i>{{ tab_5 }}</li>
                </ul>
            </div>
            <!-- tabs end -->
            <!-- tabs content start -->
            <div class="col-sm-9">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <!-- form start -->
                        <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-messengers-widget" class="form-horizontal">
                            <!-- tab 1 content start -->
                            <div class="box visible">
                                <div class="form-group">
                                    <label class="col-sm-1 control-label" for="input-status">{{ text_status }}</label>
                                    <div class="col-sm-11">
                                        <div class="toggle-button toggle-button--aava"><input id="toggleButton_status" type="checkbox" name="module_ts_messengers_widget_status"{% if (module_ts_messengers_widget_status) %}checked="checked"{% endif %}><label for="toggleButton_status" data-on-text="On" data-off-text="Off"></label><div class="toggle-button__icon"></div></div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">
                                        <label class="control-label" for="input-type">{{ text_position }}</label>
                                        <select name="module_ts_messengers_widget_settings[position]" id="input-status" class="form-control">
                                            {% if (module_ts_messengers_widget_settings['position'] == 'right') %}
                                                <option value="right" selected="selected">{{ text_right }}</option>
                                            {% else %}
                                                <option value="right">{{ text_right }}</option>
                                            {% endif %}
                                            {% if (module_ts_messengers_widget_settings['position'] == 'left') %}
                                                <option value="left" selected="selected">{{ text_left }}</option>
                                            {% else %}
                                                <option value="left">{{ text_left }}</option>
                                            {% endif %}
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">
                                        <label class="control-label" for="input-status">{{ text_padding }}</label>
                                        <div>
                                            <input type="text" name="module_ts_messengers_widget_settings[padding]" value="{{ module_ts_messengers_widget_settings['padding'] is defined ? module_ts_messengers_widget_settings['padding'] : '20' }}" class="form-control" />
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <label class="control-label" for="input-status">{{ text_padding_bottom }}</label>
                                        <div>
                                            <input type="text" name="module_ts_messengers_widget_settings[padding_bottom]" value="{{ module_ts_messengers_widget_settings['padding_bottom'] is defined ? module_ts_messengers_widget_settings['padding_bottom'] : '20' }}" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">
                                        <label class="control-label" for="input-status">{{ text_padding_mobile }}</label>
                                        <div>
                                            <input type="text" name="module_ts_messengers_widget_settings[padding_mobile]" value="{{ module_ts_messengers_widget_settings['padding_mobile'] is defined ? module_ts_messengers_widget_settings['padding_mobile'] : '20' }}" class="form-control" />
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <label class="control-label" for="input-status">{{ text_padding_bottom_mobile }}</label>
                                        <div>
                                            <input type="text" name="module_ts_messengers_widget_settings[padding_bottom_mobile]" value="{{ module_ts_messengers_widget_settings['padding_bottom_mobile'] is defined ? module_ts_messengers_widget_settings['padding_bottom_mobile'] : '20' }}" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- tab 2 content start -->
                            <div class="box">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_icon }}</label>
                                    <div class="col-sm-10">
                                        <span class="main-icon" data-toggle="modal" data-target="#main_icon" style="background: {{ module_ts_messengers_widget_settings['back_color'] }};"><i class="{{ module_ts_messengers_widget_settings['main_icon'] }}" id="main-icon" style="color:{{ module_ts_messengers_widget_settings['icon_color'] }};"></i></span>
                                        <input type="hidden" name="module_ts_messengers_widget_settings[main_icon]" id="main-icon-val" value="{{ module_ts_messengers_widget_settings['main_icon'] }}" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_no_bg }}</label>
                                    <div class="col-sm-4">
                                        <div class="toggle-button toggle-button--aava"><input id="toggleButton_no_bg" type="checkbox" name="module_ts_messengers_widget_settings[no_bg]"{% if (module_ts_messengers_widget_settings['no_bg']) %}checked="checked"{% endif %}><label for="toggleButton_no_bg" data-on-text="On" data-off-text="Off"></label><div class="toggle-button__icon"></div></div>
                                    </div>
                                    <label class="col-sm-2 control-label" for="input-type">{{ text_tips }}</label>
                                    <div class="col-sm-4">
                                        <select name="module_ts_messengers_widget_settings[tips]" id="input-status" class="form-control">
                                            {% if (module_ts_messengers_widget_settings['tips'] == '0') %}
                                                <option value="0" selected="selected">{{ text_disabled }}</option>
                                            {% else %}
                                                <option value="0">{{ text_disabled }}</option>
                                            {% endif %}
                                            {% if (module_ts_messengers_widget_settings['tips'] == '1') %}
                                                <option value="1" selected="selected">{{ text_enabled }}</option>
                                            {% else %}
                                                <option value="1">{{ text_enabled }}</option>
                                            {% endif %}
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_back_color }}</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="module_ts_messengers_widget_settings[back_color]" value="{{ module_ts_messengers_widget_settings['back_color'] is defined ? module_ts_messengers_widget_settings['back_color'] : 'rgba(37, 167, 242, 1)' }}" class="form-control rgba" />
                                    </div>
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_icon_color }}</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="module_ts_messengers_widget_settings[icon_color]" value="{{ module_ts_messengers_widget_settings['icon_color'] is defined ? module_ts_messengers_widget_settings['icon_color'] : 'rgba(255, 255, 255, 1)' }}" class="form-control rgba" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_text_color }}</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="module_ts_messengers_widget_settings[text_color]" value="{{ module_ts_messengers_widget_settings['text_color'] is defined ? module_ts_messengers_widget_settings['text_color'] : 'rgba(0, 0, 0, 1)' }}" class="form-control rgba" />
                                    </div>
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_text_hover_color }}</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="module_ts_messengers_widget_settings[text_hover_color]" value="{{ module_ts_messengers_widget_settings['text_hover_color'] is defined ? module_ts_messengers_widget_settings['text_hover_color'] : 'rgba(37, 167, 242, 1)' }}" class="form-control rgba" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_fogging }}</label>
                                    <div class="col-sm-4">
                                        <div class="toggle-button toggle-button--aava"><input id="toggleButton_fogging" type="checkbox" name="module_ts_messengers_widget_settings[fogging]"{% if (module_ts_messengers_widget_settings['fogging']) %}checked="checked"{% endif %}><label for="toggleButton_fogging" data-on-text="On" data-off-text="Off"></label><div class="toggle-button__icon"></div></div>
                                    </div>
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_fogging_color }}</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="module_ts_messengers_widget_settings[fogging_color]" value="{{ module_ts_messengers_widget_settings['fogging_color'] is defined ? module_ts_messengers_widget_settings['fogging_color'] : 'rgba(255, 255, 255, 1)' }}" class="form-control rgba" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-6">
                                        <label class="control-label" for="input-status">{{ text_icon_size_desc }}</label>
                                        <div>
                                            <div class="icon-slide"><span class="main-icon slide_1" style="background: {{ module_ts_messengers_widget_settings['back_color'] }}; width: {{ module_ts_messengers_widget_settings['desc_size'] }}px; height: {{ module_ts_messengers_widget_settings['desc_size'] }}px;"><i class="{{ module_ts_messengers_widget_settings['main_icon'] }}" style="color:{{ module_ts_messengers_widget_settings['icon_color'] }};"></i></span></div>
                                            <div class="slide_box"><input type="range" name="module_ts_messengers_widget_settings[desc_size]" min="40" max="70" value="{{ module_ts_messengers_widget_settings['desc_size'] is defined ? module_ts_messengers_widget_settings['desc_size'] : '55' }}" class="slider" id="width_1"></div>
                                        </div>
                                        <script>
                                            var slideCol_1 = document.getElementById("width_1");
                                            slideCol_1.oninput = function() {
                                                $('.slide_1').width(slideCol_1.value).height(slideCol_1.value);
                                            }
                                        </script>
                                    </div>
                                    <div class="col-sm-6">
                                        <label class="control-label" for="input-status">{{ text_icon_size_mobile }}</label>
                                        <div>
                                            <span class="icon-slide"><span class="main-icon slide_2" style="background: {{ module_ts_messengers_widget_settings['back_color'] }}; width: {{ module_ts_messengers_widget_settings['mobile_size'] }}px; height: {{ module_ts_messengers_widget_settings['mobile_size'] }}px;"><i class="{{ module_ts_messengers_widget_settings['main_icon'] }}" style="color:{{ module_ts_messengers_widget_settings['icon_color'] }};"></i></span></span>
                                            <span class="slide_box"><input type="range" name="module_ts_messengers_widget_settings[mobile_size]" min="40" max="70" value="{{ module_ts_messengers_widget_settings['mobile_size'] is defined ? module_ts_messengers_widget_settings['mobile_size'] : '55' }}" class="slider" id="width_2"></span>
                                        </div>
                                        <script>
                                            var slideCol_2 = document.getElementById("width_2");
                                            slideCol_2.oninput = function() {
                                                $('.slide_2').width(slideCol_2.value).height(slideCol_2.value);
                                            }
                                        </script>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_show_animation }}</label>
                                    <div class="col-sm-1">
                                        <div class="ts-mw-icon"><span class="callback-icon"><img id="show-animation" src="../image/catalog/ts-messengers/ts-phone.svg" alt=""></span></div>
                                    </div>
                                    <div class="col-sm-2">
                                        <select name="module_ts_messengers_widget_settings[show_animation]" id="input-show_animation" class="form-control">
                                            {% if (module_ts_messengers_widget_settings['show_animation'] == '1') %}
                                                <option value="1" selected="selected" data-type="show_animation">translate</option>
                                            {% else %}
                                                <option value="1">translate</option>
                                            {% endif %}
                                            {% if (module_ts_messengers_widget_settings['show_animation'] == '2') %}
                                                <option value="2" selected="selected">scale</option>
                                            {% else %}
                                                <option value="2">scale</option>
                                            {% endif %}
                                        </select>
                                    </div>
                                    <script>
                                        $('#input-show_animation').change(function() {
                                            var val = $('#input-show_animation').val();
                                            if (val == '1') {
                                                $('#show-animation').removeClass('scale');
                                                $('#show-animation').addClass('translate');
                                            } else if (val == '2') {
                                                $('#show-animation').removeClass('translate');
                                                $('#show-animation').addClass('scale');
                                            }
                                        });
                                        $('.panel-body #input-show_animation').trigger('change');
                                    </script>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_animation_1 }}</label>
                                    <div class="col-sm-1"><span class="main-icon animation_1" style="background: {{ module_ts_messengers_widget_settings['back_color'] }};"><i class="{{ module_ts_messengers_widget_settings['main_icon'] }}" style="color:{{ module_ts_messengers_widget_settings['icon_color'] }};"></i></span></div>
                                    <div class="col-sm-2">
                                        <div class="toggle-button toggle-button--aava"><input id="toggleButton_animation_1" type="checkbox" name="module_ts_messengers_widget_settings[animation_1]"{% if (module_ts_messengers_widget_settings['animation_1']) %}checked="checked"{% endif %}><label for="toggleButton_animation_1" data-on-text="On" data-off-text="Off"></label><div class="toggle-button__icon"></div></div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_animation_3 }}</label>
                                    <div class="col-sm-1"><span class="main-icon animation_3" style="background: {{ module_ts_messengers_widget_settings['back_color'] }};"><i class="{{ module_ts_messengers_widget_settings['main_icon'] }}" style="color:{{ module_ts_messengers_widget_settings['icon_color'] }};"></i></span><div class="pulse"></div></div>
                                    <style>.pulse:before, .pulse:after {border: 1px solid {{ module_ts_messengers_widget_settings['back_color'] }};;}</style>
                                    <div class="col-sm-2">
                                        <div class="toggle-button toggle-button--aava"><input id="toggleButton_animation_3" type="checkbox" name="module_ts_messengers_widget_settings[animation_3]"{% if (module_ts_messengers_widget_settings['animation_3']) %}checked="checked"{% endif %}><label for="toggleButton_animation_3" data-on-text="On" data-off-text="Off"></label><div class="toggle-button__icon"></div></div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-status">{{ text_animation_2 }}</label>
                                    <div class="col-sm-1">
                                        <div class="ts-mw-icon">
                                            <div class="slides"><span class="tg-icon"><img src="../image/catalog/ts-messengers/ts-telegram.svg" alt=""></span></div>
                                            <div class="slides"><span class="viber-icon"><img src="../image/catalog/ts-messengers/ts-viber.svg" alt=""></span></div>
                                            <div class="slides"><span class="whatsapp-icon"><img src="../image/catalog/ts-messengers/ts-whatsapp.svg" alt=""></span></div>
                                            <div class="slides"><span class="messenger-icon"><img src="../image/catalog/ts-messengers/ts-messenger.svg" alt=""></span></div>
                                            <div class="slides"><span class="skype-icon"><img src="../image/catalog/ts-messengers/ts-skype.svg" alt=""></span></div>
                                            <div class="slides"><span class="mail-icon"><img src="../image/catalog/ts-messengers/ts-mail.svg" alt=""></span></div>
                                            <div class="slides"><span class="vk-icon"><img src="../image/catalog/ts-messengers/ts-vk.svg" alt=""></span></div>
                                            <div class="slides"><span class="instagram-icon"><img src="../image/catalog/ts-messengers/ts-instagram.svg" alt=""></span></div>
                                            <div class="slides"><span class="jivosite-icon"><img src="../image/catalog/ts-messengers/ts-jivosite.svg" alt=""></span></div>
                                            <div class="slides"><span class="ydialog-icon"><img src="../image/catalog/ts-messengers/ts-ydialog.svg" alt=""></span></div>
                                            <div class="slides"><span class="callback-icon"><img src="../image/catalog/ts-messengers/ts-phone.svg" alt=""></span></div>
                                        </div>
                                        <script>
                                            $(document).ready(function() {
                                                var count = -1;
                                                function NextItem() {
                                                    var boxes = $('.ts-mw-icon div');
                                                    var boxLength = boxes.length - 1;
                                                    count < boxLength ? count++ : count=0;
                                                    boxes.removeClass('active').eq(count).addClass('active');
                                                }
                                                setInterval(NextItem, {{ module_ts_messengers_widget_settings['slide_speed'] }});
                                                $('.slides').first().addClass('active');
                                            });
                                        </script>
                                    </div>
                                    <div class="col-sm-2">
                                        <div class="toggle-button toggle-button--aava"><input id="toggleButton_animation_2" type="checkbox" name="module_ts_messengers_widget_settings[animation_2]"{% if (module_ts_messengers_widget_settings['animation_2']) %}checked="checked"{% endif %}><label for="toggleButton_animation_2" data-on-text="On" data-off-text="Off"></label><div class="toggle-button__icon"></div></div>
                                    </div>
                                    <div class="col-sm-4">
                                        <label class="control-label" for="input-status">{{ text_slide_speed }}</label>
                                        <div>
                                            <input type="text" name="module_ts_messengers_widget_settings[slide_speed]" value="{{ module_ts_messengers_widget_settings['slide_speed'] is defined ? module_ts_messengers_widget_settings['slide_speed'] : '3000' }}" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- tab 3 content start -->
                            <div class="box">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" for="input-type">{{ text_tips }}</label>
                                    <div class="col-sm-3">
                                        <select name="module_ts_messengers_widget_settings[main_tips]" id="input-status" class="form-control">
                                            {% if (module_ts_messengers_widget_settings['main_tips'] == '0') %}
                                                <option value="0" selected="selected">{{ text_disabled }}</option>
                                            {% else %}
                                                <option value="0">{{ text_disabled }}</option>
                                            {% endif %}
                                            {% if (module_ts_messengers_widget_settings['main_tips'] == '1') %}
                                                <option value="1" selected="selected">{{ text_enabled }}</option>
                                            {% else %}
                                                <option value="1">{{ text_enabled }}</option>
                                            {% endif %}
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" for="input-type">{{ text_tips_type }}</label>
                                    <div class="col-sm-3">
                                        <select name="module_ts_messengers_widget_settings[main_tips_type]" data-type="main_tips_type" id="input-status" class="form-control">
                                            <option value="hover" {{ module_ts_messengers_widget_settings['main_tips_type'] == 'hover' ? 'selected="selected"' : '' }} id="hover">{{ text_hover }}</option>
                                            <option value="delay" {{ module_ts_messengers_widget_settings['main_tips_type'] == 'delay' ? 'selected="selected"' : '' }} id="delay">{{ text_delay }}</option>
                                        </select>
                                    </div>
                                    <label class="col-sm-3 control-label tips-delay" style="{{ module_ts_messengers_widget_settings['main_tips_type'] == 'hover' ? '' : 'display:none;' }}" for="input-status">{{ text_delay_time }}</label>
                                    <div class="col-sm-3">
                                        <input type="text" style="{{ module_ts_messengers_widget_settings['main_tips_type'] == 'hover' ? '' : 'display:none;' }}" name="module_ts_messengers_widget_settings[delay_time]" value="{{ module_ts_messengers_widget_settings['delay_time'] is defined ? module_ts_messengers_widget_settings['delay_time'] : '5000' }}" class="form-control tips-delay" />
                                    </div>
                                    <script>$("select[data-type=main_tips_type]").change(function(){"delay"==$(this).val()?$(".tips-delay").show():$(".tips-delay").hide()}),$(function(){$("select[data-type=main_tips_type]").trigger("change")}),$(".panel-body select[data-type='main_tips_type']").trigger("change");</script>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">{{ text_text_tip }}</label>
                                    <div class="clearfix"></div>
                                    <div class="col-sm-12">
                                        <ul class="nav nav-tabs" id="tip-text">
                                            {% for language in languages %}
                                                <li>
                                                    <a href="#tab-tip-text-language{{ language['language_id'] }}" data-toggle="tab"><img src="language/{{ language['code'] }}/{{ language['code'] }}.png" title="{{ language['name'] }}" /> {{ language['name'] }}</a>
                                                </li>
                                            {% endfor %}
                                        </ul>
                                        <div class="tab-content">
                                            {% for language in languages %}
                                                <div class="tab-pane" id="tab-tip-text-language{{ language['language_id'] }}">
                                                    <input type="text" name="module_ts_messengers_widget_settings[{{ language['code'] }}][tip_text]" value="{{ module_ts_messengers_widget_settings[language['code']]['tip_text'] is not empty ? module_ts_messengers_widget_settings[language['code']]['tip_text'] : '' }}" class="form-control"/>
                                                </div>
                                            {% endfor %}
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-3">
                                        <div class="control-label">{{ text_manager }}</div>
                                        <div>
                                            <a href="" id="thumb-image" data-toggle="image" class="img-thumbnail"><img src="{{ thumb }}" alt="" title="" data-placeholder="{{ placeholder }}" class="img-responsive"/></a>
                                            <input type="hidden" name="module_ts_messengers_widget_settings[image]" value="{{ module_ts_messengers_widget_settings['image'] }}" id="input-image" />
                                        </div>
                                    </div>
                                    <div class="col-sm-9">
                                        <ul class="nav nav-tabs" id="manager-text">
                                            {% for language in languages %}
                                                <li>
                                                    <a href="#tab-manager-text-language{{ language['language_id'] }}" data-toggle="tab"><img src="language/{{ language['code'] }}/{{ language['code'] }}.png" title="{{ language['name'] }}" /> {{ language['name'] }}</a>
                                                </li>
                                            {% endfor %}
                                        </ul>
                                        <div class="tab-content">
                                            {% for language in languages %}
                                                <div class="tab-pane" id="tab-manager-text-language{{ language['language_id'] }}">
                                                    <input type="text" name="module_ts_messengers_widget_settings[{{ language['code'] }}][manager_text]" value="{{ module_ts_messengers_widget_settings[language['code']]['manager_text'] is not empty ? module_ts_messengers_widget_settings[language['code']]['manager_text'] : '' }}" class="form-control"/>
                                                </div>
                                            {% endfor %}
                                        </div>
                                    </div>
